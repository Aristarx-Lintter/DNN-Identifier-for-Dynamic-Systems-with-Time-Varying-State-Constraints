\begin{equation*}
\begin{array}{c}
z=\left[ 
\begin{array}{c}
X+d_{0}\cos \theta  \\ 
Y+d_{0}\sin \theta 
\end{array}%
\right]  \\ 
\\ 
\dot{z}=\left[ 
\begin{array}{c}
\dot{X}-d_{0}\sin \theta \dot{\theta} \\ 
\dot{Y}+d_{0}\cos \theta \dot{\theta}%
\end{array}%
\right]  \\ 
=%
\begin{array}{cc}
\cos \theta  & -\sin \theta  \\ 
\sin \theta  & \cos \theta 
\end{array}%
\begin{array}{c}
\dot{x} \\ 
\dot{y}%
\end{array}%
+%
\begin{array}{c}
-d_{0}\sin \theta \left( -\frac{1}{d_{0}}\right) \dot{y} \\ 
d_{0}\cos \theta \left( -\frac{1}{d_{0}}\right) \dot{y}%
\end{array}
\\ 
=\left[ 
\begin{array}{cc}
\cos \theta  & -\sin \theta  \\ 
\sin \theta  & \cos \theta 
\end{array}%
\right] \left[ 
\begin{array}{c}
\dot{x} \\ 
\dot{y}%
\end{array}%
\right] +\left[ 
\begin{array}{cc}
0 & \sin \theta  \\ 
0 & -\cos \theta 
\end{array}%
\right] \left[ 
\begin{array}{c}
\dot{x} \\ 
\dot{y}%
\end{array}%
\right]  \\ 
=\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \dot{x}=\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \eta _{1} \\ 
\\ 
\ddot{z}=\left[ 
\begin{array}{c}
-\sin \theta  \\ 
\cos \theta 
\end{array}%
\right] \dot{\theta}\dot{x}+\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \ddot{x} \\ 
=\left[ 
\begin{array}{c}
-\sin \theta  \\ 
\cos \theta 
\end{array}%
\right] \left( -\frac{1}{d_{0}}\right) \dot{y}\dot{x}+\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \ddot{x} \\ 
=\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \dfrac{\eta _{2}}{d_{0}}\eta _{1}+\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \dot{\eta}_{1} \\ 
\ddot{z}=\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \dfrac{\eta _{2}}{d_{0}}\eta _{1}+\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] u_{1} \\ 
\\ 
\dddot{z}=\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \dot{\theta}\dfrac{\eta _{2}}{d_{0}}\eta _{1}+\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \dfrac{\dot{\eta}_{2}}{d_{0}}\eta _{1}+\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \dfrac{\eta _{2}}{d_{0}}\dot{\eta}_{1}+\left[ 
\begin{array}{c}
-\sin \theta  \\ 
\cos \theta 
\end{array}%
\right] \dot{\theta}u_{1}+\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \dot{u}_{1} \\ 
=\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \left( -\frac{1}{d_{0}}\right) \eta _{2}\dfrac{\eta _{2}}{d_{0}}\eta
_{1}+\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \dfrac{u_{2}}{d_{0}}\eta _{1}+\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \dfrac{\eta _{2}}{d_{0}}u_{1}+\left[ 
\begin{array}{c}
-\sin \theta  \\ 
\cos \theta 
\end{array}%
\right] \left( -\frac{1}{d_{0}}\right) \eta _{2}u_{1}+\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \dot{u}_{1} \\ 
=-\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \dfrac{\eta _{2}^{2}}{d_{0}^{2}}\eta _{1}+\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \dfrac{v_{2}}{d_{0}}\eta _{1}+\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \dfrac{\eta _{2}}{d_{0}}u_{1}+\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \frac{1}{d_{0}}\eta _{2}u_{1}+\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] v_{1} \\ 
=\left[ 
\begin{array}{cc}
\cos \theta  & \dfrac{\eta _{1}}{d_{0}}\sin \theta  \\ 
\sin \theta  & -\dfrac{\eta _{1}}{d_{0}}\cos \theta 
\end{array}%
\right] \left[ 
\begin{array}{c}
v_{1} \\ 
v_{2}%
\end{array}%
\right] -\left[ 
\begin{array}{c}
\cos \theta  \\ 
\sin \theta 
\end{array}%
\right] \dfrac{\eta _{2}^{2}}{d_{0}^{2}}\eta _{1}+\left[ 
\begin{array}{c}
\sin \theta  \\ 
-\cos \theta 
\end{array}%
\right] \dfrac{2\eta _{2}}{d_{0}}u_{1}%
\end{array}%
\end{equation*}

